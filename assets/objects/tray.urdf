<?xml version="1.0" ?>
<robot name="cube.urdf">
    <link name="baseLink">
        <contact>
            <lateral_friction value="1.0"/>
            <inertia_scaling value="3.0"/>
        </contact>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <mass value="0.0"/>
            <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
        </inertial>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="cube.obj" scale=".5 .5 .01"/>
            </geometry>
            <material name="red">
                <color rgba="0.2 0.2 0.2 1"/>
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <box size=".5 .5 .01"/>
            </geometry>
        </collision>
    </link>
</robot>